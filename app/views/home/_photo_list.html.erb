<div class="row">
  <center><h1></h1></center>
  <% @photos.each do |photo| %>
    <div class="col-md-4 col-sm-6">
      <div class="inner-6">
         <div class="content">
          <h4><%= photo.avatar_file_name %><span>*</span></h4>
        </div>  
        <div class="img-box">
          <%= image_tag photo.avatar.url %>
        </div>
       
      </div>  
    </div> 
  <% end %>
</div>